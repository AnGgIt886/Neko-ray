<resources>
    <declare-styleable name="BlurImageView">
        <attr name="radius" format="integer" />
    </declare-styleable>

    <declare-styleable name="Theme">
        <attr name="changelogStyle" format="reference" />
        <attr name="changelogWindowStyle" format="reference" />
        <attr name="changelogTitleStyle" format="reference" />
        <attr name="changelogHeaderTitleStyle" format="reference" />
        <attr name="changelogHeaderDateStyle" format="reference" />
        <attr name="changelogHeaderSummaryStyle" format="reference" />
        <attr name="changelogBulletStyle" format="reference" />
        <attr name="changelogTextStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="WaveView">
		<attr name="above_wave_color" format="color"/>
		<attr name="blow_wave_color" format="color"/>
		<attr name="progress" format="integer"/>
        <attr name="wave_length" format="enum">
            <enum name="large" value="1"/>
            <enum name="middle" value="2"/>
            <enum name="little" value="3"/>
        </attr>
        <attr name="wave_height" format="enum">
            <enum name="large" value="1"/>
            <enum name="middle" value="2"/>
            <enum name="little" value="3"/>
        </attr>
        <attr name="wave_hz" format="enum">
            <enum name="fast" value="1"/>
            <enum name="normal" value="2"/>
            <enum name="slow" value="3"/>
        </attr>
	</declare-styleable>

	<declare-styleable name="Themes">
		<attr name="waveViewStyle" format="reference"/>
	</declare-styleable>

    <attr name="NoInternet_Dialog_Pendulum_airplaneColor" format="color" />
    <attr name="NoInternet_Dialog_Signal_airplaneColor" format="color" />
    <attr name="NoInternet_Dialog_Signal_topBottomShade" format="reference" />
    <attr name="NoInternet_Dialog_Signal_round" format="reference" />
    <attr name="NoInternet_Dialog_Signal_cloudColorOne" format="reference" />
    <attr name="NoInternet_Dialog_Signal_cloudColorTwo" format="reference" />
    <attr name="NoInternet_Dialog_Signal_cloudColorThree" format="reference" />

    <declare-styleable name="NekoDrawerView">
        <eat-comment />
        <attr name="sliderType">
            <enum name="scroll" value="1"/>
            <enum name="doorIn" value="2"/>
            <enum name="doorOut" value="3"/>
            <enum name="slide" value="4"/>
        </attr>
    </declare-styleable>
</resources>
